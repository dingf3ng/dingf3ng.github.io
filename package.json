{
  "name": "personal-site",
  "version": "4.0.0",
  "homepage": "https://dingf3ng.github.io/",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js",
    "build": "rimraf ./build && react-scripts build",
    "postbuild": "mv build/404.html build/404_temp.html && react-snap && mv build/404_temp.html build/404.html",
    "lint": "eslint \"**/*.js\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "test": "npx jest"
  },
  "author": "",
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "css-loader": "^7.1.2",
    "dayjs": "^1.11.19",
    "gh-pages": "^6.3.0",
    "katex": "^0.16.27",
    "postcss-loader": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.1",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^19.2.1",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "react-scripts": "^5.0.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.28.5",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@eslint/config-array": "^0.23.0",
    "@eslint/object-schema": "^3.0.0",
    "@jridgewell/sourcemap-codec": "^1.5.5",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.39.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "glob": "^13.0.0",
    "react-snap": "^1.23.0",
    "rimraf": "^6.1.2",
    "sass": "^1.95.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "crawl": true
  }
}
